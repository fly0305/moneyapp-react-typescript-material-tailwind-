{
    "name": "server2",
    "version": "0.0.0",
    "description": "TypeScript + Prisma + MySQL + Express API Server",
    "author": "",
    "license": "ISC",
    "scripts": {
      "start": "prisma generate && prisma migrate deploy && cross-env NODE_ENV=production node dist/server.js",
      "dev": "cross-env NODE_ENV=development nodemon",
      "build": "tsc && npx tsc-alias",
      "test": "jest --forceExit --detectOpenHandles",
      "lint": "eslint --ignore-path .gitignore --ext .ts src/",
      "lint:fix": "npm run lint -- --fix",
      "prisma:migrate": "prisma migrate dev --preview-feature",
      "prisma:generate": "prisma generate",
      "clean": "rm -rf node_modules/ && yarn"
    },
    "prisma": {
      "schema": "prisma/schema.prisma"
    },
