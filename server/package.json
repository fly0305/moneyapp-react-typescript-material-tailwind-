      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "cross-env": "^7.0.3",
      "dotenv": "^10.0.0",
      "envalid": "^7.1.0",
      "express": "^4.17.1",
      "helmet": "^4.6.0",
      "hpp": "^0.2.3",
      "jest": "^27.0.1",
      "jwt-decode": "^3.1.2",
      "morgan": "^1.10.0",
      "multer": "^1.4.2",
